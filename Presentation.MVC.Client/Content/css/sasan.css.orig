.profile {
    float: left;
    width: 48%;
}

    .profile fieldset {
        background-color: #FFFFFF;
        margin: 10px 10px 0px 0px;
        border: solid 1px gainsboro;
        padding: 10px;
    }

        .profile fieldset legend {
            display: none;
        }


        .profile fieldset h1,
        .profile fieldset .editlink {
            padding: 3px 0px 3px 0px;
            margin: 0px;
        }

        .profile fieldset h1 {
            font-size: 16px;
            font-weight: bold;
            text-transform: uppercase;
            color: #333333;
            display: inline;
           
            float: left;
        }

        .profile fieldset .editlink {
            float: right;
            width: 40%;
            text-align: right;
            clear: right;
        }

           .profile fieldset .editlink:hover {
               color: #6bb130;
               font-weight: bold;
        }

.display-label, .display-field {
    padding: 10px 0 5px 0;
}

.display-label {
    float: left;
    width: 120px;
    clear: left;
    color: #222;
    font-size: 12px;
}

.display-field {
    float: left;
    font-weight: bold;
    clear: right;
     font-size: 12px;
    margin-left: 5px;
}

